public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    //code here
    Boolean result;

    if((passedExam && passedAssignments) || (passedExam && passedProject) || (passedAssignments && passedProject ))
    {
        result = true;
    }
    else 
    {
        result = false;
    }

    return result;
}